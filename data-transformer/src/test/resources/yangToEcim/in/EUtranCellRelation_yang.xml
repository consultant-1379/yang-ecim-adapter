<enodeb-function>
    <eutran-cell-fdd>
        <id>myCell1</id>
        <eutran-freq-relation>
            <id>myFreqRelation1</id>
            <eutran-cell-relation>
                <id>myCellRelation1-1</id>
                <coverage-indicator>overlap</coverage-indicator>
                <neighbor-cell-ref operation="replace">/enodeb-function/eutra-network/external-enodeb-function[id='extEnb1']/external-eutran-cell-fdd[id='extCell1']</neighbor-cell-ref>
                <load-balancing>not-allowed</load-balancing>
                <s-cell-candidate>auto</s-cell-candidate>
                <s-cell-priority>7</s-cell-priority>
                <cell-individual-offset-eutran>0</cell-individual-offset-eutran>
                <q-offset-cell-eutran>0</q-offset-cell-eutran>
            </eutran-cell-relation>
            <eutran-cell-relation>
                <id>myCellRelation1-2</id>
                <coverage-indicator>none</coverage-indicator>
                <neighbor-cell-ref>/enodeb-function/eutran-cell-fdd[id='eutranCell1']</neighbor-cell-ref>
                <load-balancing>allowed</load-balancing>
                <s-cell-candidate>auto</s-cell-candidate>
                <s-cell-priority>8</s-cell-priority>
                <cell-individual-offset-eutran>-24</cell-individual-offset-eutran>
                <q-offset-cell-eutran>10</q-offset-cell-eutran>
            </eutran-cell-relation>
            <eutran-cell-relation operation="delete">
                <id>myCellRelation1-2</id>
            </eutran-cell-relation>
        </eutran-freq-relation>
    </eutran-cell-fdd>
</enodeb-function>
