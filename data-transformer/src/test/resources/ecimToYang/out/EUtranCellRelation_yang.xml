                <neighbor-cell-ref xmlns:lrtadpt="urn:rdns:com:ericsson:oammodel:ericsson-lrat-enb-adapter">/lrtadpt:enodeb-function/lrtadpt:eutran-cell-fdd[lrtadpt:id='eutranCell1']</neighbor-cell-ref>
                <load-balancing>allowed</load-balancing>
                <s-cell-candidate>auto</s-cell-candidate>
                <s-cell-priority>8</s-cell-priority>
                <cell-individual-offset-eutran>-24</cell-individual-offset-eutran>
                <q-offset-cell-eutran>10</q-offset-cell-eutran>
            </eutran-cell-relation>
        </eutran-freq-relation>
    </eutran-cell-fdd>
</enodeb-function>
