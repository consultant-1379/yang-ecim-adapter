      <ca-usage-limit>65535</ca-usage-limit>
      <ca-preemption-threshold>50</ca-preemption-threshold>
      <s-cell-act-deact-data-thres>100</s-cell-act-deact-data-thres>
      <s-cell-act-deact-data-thres-hyst>90</s-cell-act-deact-data-thres-hyst>
      <s-cell-schedule-sinr-thres>0</s-cell-schedule-sinr-thres>
      <wait-for-ca-opportunity>10000</wait-for-ca-opportunity>
      <wait-for-better-s-cell-rep>1000</wait-for-better-s-cell-rep>
      <wait-for-additional-s-cell-opportunity>10000</wait-for-additional-s-cell-opportunity>
      <ca-rate-adjust-coeff>10</ca-rate-adjust-coeff>
